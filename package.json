{
  "name": "next-smrt",
  "version": "0.0.1",
  "author": "Chris Sprance <chrissprance@gmail.com>",
  "scripts": {
    "dev": "nodemon server/index.ts",
    "start": "cross-env NODE_ENV=production node .next/production-server/index.js",
    "build": "next build && tsc --project tsconfig.server.json",
    "export": "next export",
    "analyze": "cross-env NODE_ENV=production ANALYZE=true next build",
    "repl": "ts-node -P working/tsconfig.json working/repl.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^2.0.1",
    "axios": "^0.18.0",
    "next": "^6.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-responsive": "^4.1.0",
    "redux": "^3.7.2",
    "redux-persist": "^4.9.1",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.6",
    "sweetalert": "^2.1.0",
    "typesafe-actions": "^2.0.3",
    "yup": "^0.25.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.48",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.3",
    "@types/next": "^2.4.7",
    "@types/node": "^8.10.14",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.3.14",
    "@types/react-redux": "^6.0.0",
    "@zeit/next-css": "^0.2.0",
    "@zeit/next-typescript": "^1.1.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "cssnano": "^3.10.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fs-extra": "^6.0.1",
    "jest": "^22.4.3",
    "nextjs-export-path-map": "^0.1.2",
    "nodemon": "^1.18.3",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "redux-logger": "^3.0.6",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "typescript-babel-jest": "^1.0.5",
    "webpack-bundle-analyzer": "^2.11.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true,
        "tsConfigFile": "jest.tsconfig.json"
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "enzyme.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/enzyme.js",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
